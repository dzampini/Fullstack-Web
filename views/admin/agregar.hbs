
         
    <form action="/admin/login_sucess/agregar" method="post" enctype="multipart/form-data">
        <div class="mb-3"><label  class="form-label"></label><input type="text" class="form-control" id="exampleFormControlInput1" placeholder="Nombres" name="nombre"/></div>
        <div class="mb-3"><label  class="form-label"></label><input type="text" class="form-control" id="exampleFormControlInput1" placeholder="Apellidos" name="apellido"/></div>
        <div class="mb-3"><label  class="form-label"></label><input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Email" name="mail"/></div>
        <div class="mb-3"><label>Imagen:<input type="file" name="imagen"/></label></div>
        <div class="containerbutton" style="margin: top 50px;"><div class="row"><button type="submit" class="btn btn-primary">Guardar</button></div></div>
